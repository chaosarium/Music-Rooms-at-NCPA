<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Practise Room Signup</title>
  <link rel="icon" href="/favicons/Image002.png">
  <link rel="stylesheet" href="/css/style.css">
  <link rel="stylesheet" href="/css/bootstrap.css">
</head>
<body>

  <div class="container">
    <h1 class="mt-4">Practise Room Signup <sup>beta</sup></h1>
    <table>
      <thead>
        <tr>
          <th rowspan="2">Room</th>
          <th colspan="3">19:00-19:30</th>
          <th colspan="3">19:30-20:00</th>
          <th colspan="3">20:00-20:30</th>
          <th colspan="3">20:30-21:00</th>
        </tr>
        <tr>
          <td>Full Name</td>
          <td>Grade</td>
          <td>Remark</td>
          <td>Full Name</td>
          <td>Grade</td>
          <td>Remark</td>
          <td>Full Name</td>
          <td>Grade</td>
          <td>Remark</td>
          <td>Full Name</td>
          <td>Grade</td>
          <td>Remark</td>
        </tr>
      </thead>
      <tbody>

        <% for (var i = 0; i < dataList.length; i++) { %>
          <% if (dataList[i].Time == "19:00-19:10") { %>
            <tr>
              <td><%= dataList[i].FirstName + " " + dataList[i].LastName %></td>
              <td><%= dataList[i].StudentID %></td>
              <td><%= dataList[i].Year %></td>
            </tr>
          <% } %>
        <% } %>


        <tr>
          <th>MH102</th>

          <td onclick="window.location.replace('/MH102/T19001930')"><%= organisedData.MH102.T19001930.fullName %></td>
          <td onclick="window.location.replace('/MH102/T19001930')"><%= organisedData.MH102.T19001930.grade %></td>
          <td onclick="window.location.replace('/MH102/T19001930')"><%= organisedData.MH102.T19001930.remarkStatus %></td>

          
          <td onclick="window.location.replace('/MH102/T19302000')"><%= organisedData.MH102.T19302000.fullName %></td>
          <td onclick="window.location.replace('/MH102/T19302000')"><%= organisedData.MH102.T19302000.grade %></td>
          <td onclick="window.location.replace('/MH102/T19302000')"><%= organisedData.MH102.T19302000.remarkStatus %></td>
          <td onclick="window.location.replace('/MH102/T20002030')"><%= organisedData.MH102.T20002030.fullName %></td>
          <td onclick="window.location.replace('/MH102/T20002030')"><%= organisedData.MH102.T20002030.grade %></td>
          <td onclick="window.location.replace('/MH102/T20002030')"><%= organisedData.MH102.T20002030.remarkStatus %></td>
          <td onclick="window.location.replace('/MH102/T20302100')"><%= organisedData.MH102.T20302100.fullName %></td>
          <td onclick="window.location.replace('/MH102/T20302100')"><%= organisedData.MH102.T20302100.grade %></td>
          <td onclick="window.location.replace('/MH102/T20302100')"><%= organisedData.MH102.T20302100.remarkStatus %></td>
        </tr>

      </tbody>
    </table>

    <p class="mt-4"><a href="/cancel">link cancel request</a></p>
    <p><a href="/admin">link admin</a></p>

    <h3 class="mt-5">Debug info:</h3>
    <p><small class="text-muted">from index.ejs</small></p>
    <p><small>Data: <%= JSON.stringify(organisedData) %></small></p>


  </div>


  <!-- JQuery -->
  <script type="text/javascript" src="/js/jquery-3.4.0.min.js"></script>
  <!-- Bootstrap tooltips -->
  <script type="text/javascript" src="/js/popper.min.js"></script>
  <!-- Bootstrap core JavaScript -->
  <script type="text/javascript" src="/js/bootstrap.min.js"></script>
  <script src="/js/main.js"></script>
</body>
</html>